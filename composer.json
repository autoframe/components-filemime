{
    "type": "library",
    "name": "autoframe/components-filemime",
    "description": "Core File Mime Utils Autoframe Framework",
    "license": "MIT",
    "homepage": "https://github.com/autoframe",
    "require": {
        "php": "^7.4 || ^8.0",
        "autoframe/components-exception": "^1.0 || ^2.0"
    },
    "version": "2.0.0",
    "authors": [
        {
            "name": "Nistor Alexandru Marius",
            "homepage": "https://autoframe.ro",
            "role": "Developer"
        }
    ],
    "keywords": ["mime", "file" ],
    "autoload": {
        "psr-4": {
            "Autoframe\\Components\\FileMime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unit\\": "Tests/Unit"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10",
        "symfony/var-dumper": "v5.4.24 || v6.2.11"
    },
    "optimize-autoloader": true
}
